("Email") = EmailFld.Text		  'd.Value("Application") = "My Application"		  for i As Integer = 0 to KeyBox.ListCount - 1		    dim key As String = Keybox.Cell(i, 0)		    dim value As String = Keybox.Cell(i, 1)		    if key <> "" and value <> "" Then		      d.Value(key) = value		    end if		  next		  		  'dim data As String = a.LicenseDataForDictionary(d)		  dim ext As String = extFld.Text		  if ext = "" Then ext = "plist"		  dim fName As String = NameFld.Text.Trim + "." + ext		  dim file As FolderItem = self.SaveLoc.Child(fName)		  		  if not a.WriteLicenseFileForDictionary(d, file) Then		    MsgBox "There was a problem creating the licence file." + EndOfLine + a.LastError		  Else		    MsgBox "The license file was saved at: " + file.ShellPath		  End If		  		End Sub	#tag EndEvent#tag EndEvents#tag Events SaveLocBtn	#tag Event		Sub Action()		  dim dlg As new SelectFolderDialog		  dlg.PromptText = "Location for saving license files"		  dlg.InitialDirectory = SpecialFolder.Documents		  self.SaveLoc = dlg.ShowModalWithin(self)		  		  ShowSaveLocation		  		End Sub	#tag EndEvent#tag EndEvents#tag Events PublicKeyFld	#tag Event		Sub TextChange()		  if PrivateKeyFld.Text.Trim <> "" Then		    ObfuscatedFld.Text = ObfuscateKey		  end if		  stateChanged		End Sub	#tag EndEvent#tag EndEvents#tag Events KeyBox	#tag Event		Sub Change()		  MinusBtn.Enabled = me.ListIndex >= 0		End Sub	#tag EndEvent	#tag Event		Sub DoubleClick()		  dim column As Integer = me.ColumnFromXY(me.MouseX, me.MouseY)		  if column >= 0 Then		    me.EditCell(me.ListIndex, column)		  end if		End Sub	#tag EndEvent	#tag Event		Function CellKeyDown(row as Integer, column as Integer, key as String) As Boolean		  if key.Asc = 9 Then		    if column = 0 Then		      me.EditCell(row, 1)		    Else		      me.EditCell(row, 0)		    End If		    		  Elseif key.Asc = 3 or key.Asc = 13 Then		    self.SetFocus		  End If		End Function	#tag EndEvent#tag EndEvents#tag Events MinusBtn	#tag Event		Sub Action()		  dim row As Integer = Keybox.ListIndex		  		  if row < 0 Then Return		  		  KeyBox.RemoveRow row		End Sub	#tag EndEvent#tag EndEvents#tag Events PlusBtn	#tag Event		Sub Action()		  keybox.AddRow ""		  		  dim row As Integer = keybox.LastIndex		  keybox.EditCell(row, 0)		End Sub	#tag EndEvent#tag EndEvents#tag Events extFld	#tag Event		Sub Open()		  me.Text = "plist"		  		End Sub	#tag EndEvent#tag EndEvents#tag Events CreateModuleBtn	#tag Event		Sub Action()		  dim n As String = GetInfoDlg.NumInit("Minimum number of methods to create (1 - 8)", "4").Trim		  if n = "" Then Return		  		  if n = "0" Then		    Beep		    Return		  end if		  		  		  CreateObscurePubKeyCode PublicKeyFld.Text, val(n)		  		  		End Sub	#tag EndEvent#tag EndEvents