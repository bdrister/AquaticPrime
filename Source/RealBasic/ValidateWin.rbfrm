n		    TestLicense obj.folderItem		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  static anyFileType as new FileType		  anyFileType.Name = "special/any"		  anyFileType.MacType = "????"		  anyFileType.Extensions = ""		  		  self.AcceptFileDrop anyFileType		  		End Sub	#tag EndEvent	#tag MenuHandler		Function FileClose() As Boolean Handles FileClose.Action			self.Close			return true		End Function	#tag EndMenuHandler	#tag Method, Flags = &h0		Sub TestLicense(licenseFile as folderItem)		  dim licenseValidator As new AquaticPrime(PublicKeyFld.text)		  		  if licenseValidator.LastError <> "" then		    		    beep		    Listbox1.visible = false		    HashTxt.visible = false		    HashFld.Visible = False		    DropTxt.MultiLine = true		    DropTxt.text = "PubKey Error: "+EndOfLine+EndOfLine+licenseValidator.LastError		    DropTxt.visible = true		    		  else		    		    dim licenseDict as dictionary = licenseValidator.DictionaryForLicenseFile(licenseFile)		    		    if licenseDict = nil then		      beep		      Listbox1.visible = false		      HashTxt.visible = false		      HashFld.Visible = False		      DropTxt.MultiLine = true		      DropTxt.text = "License is NOT VALID:"+EndOfLine+EndOfLine+licenseValidator.LastError		      DropTxt.visible = true		    else		      Listbox1.deleteAllRows		      HashFld.text = ""		      for i as integer = 0 to licenseDict.count-1		        Listbox1.addRow licenseDict.key(i).StringValue		        Listbox1.cell(listbox1.lastIndex, 1) = licenseDict.value(licenseDict.key(i))		      next		      HashFld.text = Uppercase(licenseValidator.hash)		      DropTxt.MultiLine = false		      DropTxt.visible = false		      HashTxt.visible = true		      HashFld.Visible = True		      listbox1.visible = true		    end if		    		  end		End Sub	#tag EndMethod#tag EndWindowCode#tag Events PublicKeyFld	#tag Event		Sub TextChange()		  App.Prefs.Value ("Validation PubKey") = me.Text		End 